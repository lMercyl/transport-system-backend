{
  "version": "0.0.1",
  "builds": [
    {
      "src": "dist/main.ts",
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/maint.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ],
  "github": {
    "enabled": false,
    "silent": true
  }
}